<div class="container">
  <h1>User Created!</h1>
  <p>
    <strong>First Name:</strong>
    <%= @user.first_name %>
  </p>
  
  <p>
    <strong>Last Name:</strong>
    <%= @user.last_name %>
  </p>
  
  <p>
    <strong>Email:</strong>
    <%= @user.email %>
  </p>
  
  <p>
    <strong>Password:</strong>
    <%= @user.password %>
  </p>
    
  <%= link_to 'Edit User', edit_user_path(@user) %> |
  <%= link_to 'Back', users_path %>
</div>